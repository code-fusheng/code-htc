<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xyz.fusheng.code.htc.core.mapper.DeviceMapper">

    <select id="selectCarCameraRefByCamera" resultType="xyz.fusheng.code.htc.model.entity.DeviceCarCameraRef">
        select * from patrol_device_car_camera_ref pdccr
        where pdccr.camera_id = #{cameraId}
    </select>

</mapper>